{
  "name": "gamification-twitter",
  "version": "1.0.0",
  "description": "Gamification Twitter Connector",
  "repository": {
    "type": "git",
    "url": "https://github.com/Meeds-io/gamification-twitter"
  },
  "scripts": {
    "watch": "webpack --config webpack.watch.js --progress --colors --watch -d",
    "format": "prettier --config ./.prettierrc --write ./src/main/webapp/vue-app/*.js ./src/main/webapp/vue-app/js/*.js",
    "lint": "eslint --fix ./src/main/webapp/vue-app/**/*.js ./src/main/webapp/vue-app/**/*.vue ./src/main/webapp/vue-app/components/*.vue",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "build": "webpack --config webpack.prod.js --mode production"
  },
  "author": "Meeds.io",
  "license": "LGPL",
  "devDependencies": {
    "babel-loader": "^8.3.0",
    "css-loader": "^0.28.11",
    "eslint": "^8.12.0",
    "eslint-config-meedsio": "^1.0.5",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-webpack-plugin": "^3.1.1",
    "vue": "^2.6.14",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
